{
    "name": "solitus0/alice-generator",
    "description": "Recursively generate Alice fixtures from existing objects",
    "keywords": [
        "alice",
        "fixture",
        "data",
        "test",
        "generator",
        "orm",
        "doctrine"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ernestas Pilsudskis",
            "email": "ernestas.pilsudskis@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/yaml": ">=5.0",
        "doctrine/common": "*",
        "jms/metadata": "^2.0",
        "doctrine/orm": ">=2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "nelmio/alice": "^3.3",
        "rector/rector": "^2.0",
        "symplify/easy-coding-standard": "^12.5",
        "phpstan/phpstan": "^2.1.6",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "spaze/phpstan-disallowed-calls": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "Solitus0\\AliceGenerator\\": "src/AliceGenerator"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Solitus0\\AliceGenerator\\Tests\\": "tests/AliceGenerator"
        }
    }
}
